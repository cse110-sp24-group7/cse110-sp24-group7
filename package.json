{
  "name": "devjournal",
  "version": "1.0.0",
  "description": "Akatsuki 7's CSE 110 Developer Journal",
  "main": "./source/project/scripts/main.js",
  "author": "Akatsuki 7",
  "type": "commonjs",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-dmg": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.44.1",
    "babel-jest": "^29.7.0",
    "electron": "^30.0.8",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.0.1",
    "jsdoc": "^4.0.3",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "puppeteer": "^22.10.0"
  },
  "scripts": {
    "test:unit": "jest --maxWorkers=1",
    "prettier:fix": "prettier --write --ignore-path .prettierignore \"./source/project/**/*.{html,css,js}\" \"./specs/**/*.md\"",
    "prettier:check": "prettier --check --ignore-path .prettierignore \"./source/project/**/*.{html,css,js}\" \"./specs/**/*.md\"",
    "jsdoc": "jsdoc -c ./source/project/config/jsdoc.config.json",
    "eslint:check": "eslint \"./source/project/**/*.js\" \"./source/project/components/**/*.js\" \"./source/project/scripts/**/*.js\"",
    "eslint:fix": "eslint --fix \"./source/project/**/*.js\" \"./source/project/components/**/*.js\" \"./source/project/scripts/**/*.js\" --debug",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "transform": {},
    "verbose": true,
    "testPathIgnorePatterns": [
      "./source/project/__tests__/E2E",
      "./source/project/__tests__/playwright",
      "./source/project/__tests__/fileSource"
    ]
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "sqlite3": "^5.1.7"
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": true
  }
}
